  clvapit:hasRankOrder "3"^^xsd:int;
  skos:prefLabel "Erogazione di servizi collettivi"@it, "Provision of services to the community as a whole"@en;
  skos:definition "This group includes foreign affairs, defence, justice, public order and safety activities."@en,
    "Questo gruppo include: affari esteri, difesa nazionale, giustizia e attività giudiziarie, ordine pubblico e sicurezza nazionale."@it;
  skos:narrower :8421, :8422, :8423, :8424, :8425 .

