  skos:notation "91.3";
  clvapit:hasRankOrder "3"^^xsd:int;
  skos:prefLabel "Conservazione, restauro e altre attività di supporto al patrimonio culturale"@it,
    "Conservation, restoration and other support activities for cultural heritage"@en;
  skos:narrower :9130 .

